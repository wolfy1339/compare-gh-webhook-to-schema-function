{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "release$prereleased",
  "title": "release prereleased event",
  "type": "object",
  "properties": {
    "action": {
      "type": "string",
      "enum": [
        "prereleased"
      ]
    },
    "enterprise": {
      "$ref": "common/enterprise-webhooks.schema.json"
    },
    "installation": {
      "$ref": "common/simple-installation.schema.json"
    },
    "organization": {
      "$ref": "common/organization-simple-webhooks.schema.json"
    },
    "release": {
      "description": "The [release](https://docs.github.com/rest/releases/releases/#get-a-release) object.",
      "type": "object",
      "required": [
        "assets",
        "assets_url",
        "author",
        "body",
        "created_at",
        "draft",
        "html_url",
        "id",
        "name",
        "node_id",
        "prerelease",
        "published_at",
        "tag_name",
        "tarball_url",
        "target_commitish",
        "upload_url",
        "url",
        "zipball_url"
      ],
      "properties": {
        "assets": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "object",
                "description": "Data related to a release.",
                "required": [
                  "url",
                  "browser_download_url",
                  "id",
                  "node_id",
                  "name",
                  "label",
                  "state",
                  "digest",
                  "content_type",
                  "size",
                  "download_count",
                  "created_at",
                  "updated_at"
                ],
                "properties": {
                  "browser_download_url": {
                    "type": "string",
                    "format": "uri"
                  },
                  "content_type": {
                    "type": "string"
                  },
                  "created_at": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "download_count": {
                    "type": "integer"
                  },
                  "id": {
                    "type": "integer"
                  },
                  "label": {
                    "oneOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "name": {
                    "description": "The file name of the asset.",
                    "type": "string"
                  },
                  "node_id": {
                    "type": "string"
                  },
                  "size": {
                    "type": "integer"
                  },
                  "digest": {
                    "oneOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "state": {
                    "description": "State of the release asset.",
                    "type": "string",
                    "enum": [
                      "uploaded"
                    ]
                  },
                  "updated_at": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "uploader": {
                    "oneOf": [
                      {
                        "type": "object",
                        "required": [
                          "login",
                          "id"
                        ],
                        "properties": {
                          "avatar_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "deleted": {
                            "type": "boolean"
                          },
                          "email": {
                            "oneOf": [
                              {
                                "type": "string"
                              },
                              {
                                "type": "null"
                              }
                            ]
                          },
                          "events_url": {
                            "type": "string",
                            "format": "uri-template"
                          },
                          "followers_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "following_url": {
                            "type": "string",
                            "format": "uri-template"
                          },
                          "gists_url": {
                            "type": "string",
                            "format": "uri-template"
                          },
                          "gravatar_id": {
                            "type": "string"
                          },
                          "html_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "id": {
                            "type": "integer"
                          },
                          "login": {
                            "type": "string"
                          },
                          "name": {
                            "type": "string"
                          },
                          "node_id": {
                            "type": "string"
                          },
                          "organizations_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "received_events_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "repos_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "site_admin": {
                            "type": "boolean"
                          },
                          "starred_url": {
                            "type": "string",
                            "format": "uri-template"
                          },
                          "subscriptions_url": {
                            "type": "string",
                            "format": "uri"
                          },
                          "type": {
                            "type": "string",
                            "enum": [
                              "Bot",
                              "User",
                              "Organization"
                            ]
                          },
                          "url": {
                            "type": "string",
                            "format": "uri"
                          }
                        }
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "url": {
                    "type": "string",
                    "format": "uri"
                  }
                }
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "assets_url": {
          "type": "string",
          "format": "uri"
        },
        "author": {
          "oneOf": [
            {
              "type": "object",
              "required": [
                "login",
                "id"
              ],
              "properties": {
                "avatar_url": {
                  "type": "string",
                  "format": "uri"
                },
                "deleted": {
                  "type": "boolean"
                },
                "email": {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "null"
                    }
                  ]
                },
                "events_url": {
                  "type": "string",
                  "format": "uri-template"
                },
                "followers_url": {
                  "type": "string",
                  "format": "uri"
                },
                "following_url": {
                  "type": "string",
                  "format": "uri-template"
                },
                "gists_url": {
                  "type": "string",
                  "format": "uri-template"
                },
                "gravatar_id": {
                  "type": "string"
                },
                "html_url": {
                  "type": "string",
                  "format": "uri"
                },
                "id": {
                  "type": "integer"
                },
                "login": {
                  "type": "string"
                },
                "name": {
                  "type": "string"
                },
                "node_id": {
                  "type": "string"
                },
                "organizations_url": {
                  "type": "string",
                  "format": "uri"
                },
                "received_events_url": {
                  "type": "string",
                  "format": "uri"
                },
                "repos_url": {
                  "type": "string",
                  "format": "uri"
                },
                "site_admin": {
                  "type": "boolean"
                },
                "starred_url": {
                  "type": "string",
                  "format": "uri-template"
                },
                "subscriptions_url": {
                  "type": "string",
                  "format": "uri"
                },
                "type": {
                  "type": "string",
                  "enum": [
                    "Bot",
                    "User",
                    "Organization"
                  ]
                },
                "url": {
                  "type": "string",
                  "format": "uri"
                },
                "user_view_type": {
                  "type": "string"
                }
              }
            },
            {
              "type": "null"
            }
          ]
        },
        "body": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "created_at": {
          "oneOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "null"
            }
          ]
        },
        "discussion_url": {
          "type": "string",
          "format": "uri"
        },
        "draft": {
          "description": "Whether the release is a draft or published",
          "type": "boolean"
        },
        "html_url": {
          "type": "string",
          "format": "uri"
        },
        "id": {
          "type": "integer"
        },
        "name": {
          "oneOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ]
        },
        "node_id": {
          "type": "string"
        },
        "prerelease": {
          "description": "Whether the release is identified as a prerelease or a full release.",
          "type": "boolean",
          "enum": [
            true
          ]
        },
        "published_at": {
          "oneOf": [
            {
              "type": "string",
              "format": "date-time"
            },
            {
              "type": "null"
            }
          ]
        },
        "reactions": {
          "type": "object",
          "required": [
            "url",
            "total_count",
            "+1",
            "-1",
            "laugh",
            "confused",
            "heart",
            "hooray",
            "eyes",
            "rocket"
          ],
          "properties": {
            "+1": {
              "type": "integer"
            },
            "-1": {
              "type": "integer"
            },
            "confused": {
              "type": "integer"
            },
            "eyes": {
              "type": "integer"
            },
            "heart": {
              "type": "integer"
            },
            "hooray": {
              "type": "integer"
            },
            "laugh": {
              "type": "integer"
            },
            "rocket": {
              "type": "integer"
            },
            "total_count": {
              "type": "integer"
            },
            "url": {
              "type": "string",
              "format": "uri"
            }
          }
        },
        "tag_name": {
          "description": "The name of the tag.",
          "type": "string"
        },
        "tarball_url": {
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
        },
        "target_commitish": {
          "description": "Specifies the commitish value that determines where the Git tag is created from.",
          "type": "string"
        },
        "upload_url": {
          "type": "string",
          "format": "uri-template"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "zipball_url": {
          "oneOf": [
            {
              "type": "string",
              "format": "uri"
            },
            {
              "type": "null"
            }
          ]
        }
      }
    },
    "repository": {
      "$ref": "common/repository-webhooks.schema.json"
    },
    "sender": {
      "$ref": "common/simple-user.schema.json"
    }
  },
  "required": [
    "action",
    "release",
    "repository"
  ],
  "additionalProperties": false
}
